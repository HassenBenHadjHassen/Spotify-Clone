{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "google",
    "prettier",
    "plugin:sonarjs/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "sonarjs",
    "prettier"
  ],
  "rules": {
    "require-jsdoc": "off",
    "no-unused-vars": "off",
    "new-cap": "off",
    "@typescript-eslint/no-unused-vars": "error"
  }
}